<section class="MenuItem-display"> 
    <% @menu_items.each do |menu_item| %>
        <div class="MenuItem-view">
            <% default_image_url = "data:image/jpeg;base64," %>
            <% image_url = menu_item.image_url.presence || default_image_url %>
            <img src='<%= image_url %>' class="MenuItem-image" alt='Image of <%= menu_item.menu_item_name %>' />
            <p class="MenuItem-name"> <%= menu_item.menu_item_name %> </p>
            <div class="MenuItem-display-end">
                <p class="MenuItem-price">  ₹ <%= menu_item.menu_item_price %> </p>
                
                <% if @current_user.is_owner(@current_user.id) %>
                    <div class="MenuItem-options">    
                        <%= link_to "Edit",
                                    edit_menu_item_path(menu_item, :current_menu_id => @current_menu_id) %>
                    
                        <%= link_to "Remove",
                                    menu_item_path(menu_item, :current_menu_id => @current_menu_id), method: :delete, data: { confirm: "Are you sure?" } %>
                    </div>
                <% end %>
            </div>
        </div>
       
        <hr> 
    <% end %>
</section>